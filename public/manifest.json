{
    "name": "YouTime",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "windows"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "commands": {
        "save-timestamp": {
            "suggested_key": "Alt+Shift+T",
            "description": "Save Timestamp"
        },
        "open-vault": {
            "suggested_key": "Alt+Shift+V",
            "description": "Open Vault"
        }
    }
}
